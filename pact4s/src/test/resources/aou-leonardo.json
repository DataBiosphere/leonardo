{
  "provider": {
    "name": "leonardo"
  },
  "consumer": {
    "name": "aou"
  },
  "interactions": [
    {
      "description": "a request to get that app",
      "request": {
        "method": "GET",
        "path": "/api/google/v1/apps/googleProject/appname"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "diskName": "disk-123",
          "appName": "sample-cromwell-study",
          "appType": "CROMWELL",
          "cloudContext": {},
          "errors": [],
          "status": "RUNNING"
        },
        "matchingRules": {
          "body": {
            "$.appName": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.status": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.diskName": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.appType": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            },
            "$.cloudContext.cloudprovider": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "there is an app in a Google project"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.10"
    }
  }
}